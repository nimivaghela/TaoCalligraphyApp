        <item name="android:windowLightStatusBar">true</item>
        <item name="android:windowContentTransitions">true</item>
    </style>

    <style name="ExplodeTransition" parent="CustomThemeDarkStatusBar">
        <item name="android:windowEnterTransition">@transition/explode_animation</item>
        <item name="android:windowExitTransition">@transition/explode_animation</item>
